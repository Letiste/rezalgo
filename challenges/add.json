{
  "$schema": "../challenges.schema.json",
  "name": "Addition",
  "description": "Create a function `add` that return the **sum** of the two numbers passed as parameters",
  "examples": [
    {
    "input": "a = 2, b = 2",
    "output": "4",
    "explanation": "really ?"
  },
  {
    "input": "a = 4, b = 15",
    "output": "19"
  }
],
  "functionName": "add",
  "params": ["a", "b"],
  "tests": [
    {
      "inputs": ["2", "2"],
      "expected": "4"
    },
    {
      "inputs": ["2", "3"],
      "expected": "5"
    }
  ]
}